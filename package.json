{
  "name": "colisbridge",
  "version": "1.0.0",
  "description": "Application de gestion de colis transfrontaliers entre la Chine et l'Afrique",
  "main": "src/app.js",
  "scripts": {
    "server": "nodemon src/app.js",
    "client": "cd client && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.2",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0"
  }
}
